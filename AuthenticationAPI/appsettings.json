{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    //SecretKey set as environmentvariable
    "Issuer": "MyAppBackend",
    "Audience": "MyAppFrontend",
    "AccessTokenExpiration": 10,
    "RefreshTokenExpiration": 50
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LoggingProvider": "Serilog"
  },
  "AllowedHosts": "*"

}




